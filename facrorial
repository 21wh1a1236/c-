#include<iostream>
using namespace std;
int factorial(int n)
{
        if(n > 1)
                return n * factorial(n - 1);
        else
                return 1;
}
int main()
{
        int n,x;
        cout << "Enter n: ";
        cin >> n;
        x = factorial(n);
        cout << "Factorial of " << x <<endl;
        return 0;
}
