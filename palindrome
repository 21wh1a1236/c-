include<iostream>
using namespace std;
int main()
{
        int n,rem,sum=0,temp;
        cout<<"enter n:";
        cin>>n;
        temp=n;
        while(n>0){
                rem = n%10;
                sum = (sum*10)+rem;
                n=n/10;
        }
        if(temp==sum)
                cout<<"palindrome"<<endl;
        else
                cout<<"Not a palindrome"<<endl;
        return 0;
}
