#include<iostream>
using namespace std;
int main()
{
        int sum = 0,n;
        int *a = new int[n];
        cout<<"Enter n:"<<endl;
        cin>>n;
        for(int i=0;i<n;i++)
        {
                cin>>a[i];
        }
        for(int i=0;i<n;i++)
        {
                sum += a[i];
        }
        cout<<"Total sum: "<<sum<<endl;
        delete(a);
        return 0;
}
